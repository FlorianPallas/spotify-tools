{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "@nuxtjs/eslint-config-typescript",
    "prettier",
    "prettier/vue",
    "plugin:prettier/recommended",
    "plugin:nuxt/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "no-unused-vars": "warn",
    "linebreak-style": ["error", "unix"],
    "vue/no-v-html": "off"
  }
}
